      <!-- footer area end-->
    </div>
    <!-- page container area end -->
    <xpath expr="." position="inside">
      <script src="/Arope-spaces/static/src/js/popper.min.js"></script>
      <script src="/Arope-spaces/static/src/js/owl.carousel.min.js"></script>
      <script src="/Arope-spaces/static/src/js/metisMenu.min.js"></script>
      <script src="/Arope-spaces/static/src/js/jquery.slimscroll.min.js"></script>
      <script src="/Arope-spaces/static/src/js/jquery.slicknav.min.js"></script>
      <script src="/Arope-spaces/static/src/js/amcharts.js"></script>
      <script src="/Arope-spaces/static/src/js/serial.js"></script>
      <script src="/Arope-spaces/static/src/js/pie.js"></script>
      <script src="/Arope-spaces/static/src/js/export.min.js"></script>
      <script src="/Arope-spaces/static/src/js/light.js"></script>
      <script src="/Arope-spaces/static/src/js/plugins.js"></script>
      <script src="/Arope-spaces/static/src/js/scripts.js"></script>
    </xpath>
  </t>

