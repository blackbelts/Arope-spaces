<odoo>
    <data>
        <record id="hr_action_dashboard" model="ir.actions.client">
            <field name="name">Dashboard</field>
            <field name="tag">hr_dashboard</field>
        </record>
        <menuitem id="menu_hrms_dashboard" name="Dashboard" action="hr_action_dashboard"
            web_icon="Arope-spaces,static/description/dashboard_icon.png" sequence="-100" />
    </data>
    <template id="assets_backend_dashboard" name="Hr Dashboard assets inherit" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/Arope-spaces/static/src/css/hrms_dashboard.css" />
            <script type="text/javascript" src="/Arope-spaces/static/src/js/hrms_dashboard.js" />
            <link rel="stylesheet" href="/Arope-spaces/static/src/css/lib/nv.d3.css"/>
            <script type="text/javascript" src="/Arope-spaces/static/src/js/lib/d3.min.js"/>
        </xpath>
    </template>
</odoo>